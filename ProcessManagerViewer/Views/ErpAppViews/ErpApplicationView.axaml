<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="using:ProcessManagerViewer"
             xmlns:rxui="http://reactiveui.net"
             xmlns:vm="using:ProcessManagerViewer.ViewModels.ErpAppViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ProcessManagerViewer.Views.ErpAppViews.ErpApplicationView"
             x:DataType="vm:ErpApplicationViewModel">
    <DockPanel>
        <TextBlock Text="ERP Application"
                   DockPanel.Dock="Top"/>
        <rxui:RoutedViewHost Router="{Binding Router}">
            <rxui:RoutedViewHost.ViewLocator>
                <local:ViewLocator />
            </rxui:RoutedViewHost.ViewLocator>
        </rxui:RoutedViewHost>
    </DockPanel>
</UserControl>
