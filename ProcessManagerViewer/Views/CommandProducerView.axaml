<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:rxui="http://reactiveui.net"
             xmlns:vm="using:ProcessManagerViewer.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ProcessManagerViewer.Views.CommandProducerView"
             x:DataType="vm:CommandProducerViewModel">
    <StackPanel Orientation="Vertical"
                VerticalAlignment="Stretch"
                HorizontalAlignment="Stretch">
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Number of Clicks: " />
            <TextBlock Text="{Binding NumberOfClicks}" />
        </StackPanel>
        <Button Command="{Binding OnButtonPressedCommand}"
                Content="Click me!" />
    </StackPanel>
</UserControl>
